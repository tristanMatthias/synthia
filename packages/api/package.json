    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "geoip-lite": "^1.3.7",
    "graphql": "^14.4.2",
    "graphql-type-json": "^0.3.0",
    "helmet": "^3.20.0",
    "history": "^4.9.0",
    "http-status": "^1.3.2",
    "jsonwebtoken": "^8.5.1",
    "lerna": "^3.16.4",
    "murmurhash3js": "^3.0.1",
    "pg": "^7.12.0",
    "reflect-metadata": "^0.1.13",
    "request-promise-native": "^1.0.7",
    "sequelize": "^5.12.3",
    "sequelize-typescript": "^1.0.0-alpha.9",
