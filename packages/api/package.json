  "repository": "https://github.com/tristanMatthias/synthia",
  "author": "Tristan Matthias <hello@tristanmatthias.com>",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/server/index",
    "dev": "tsc -w & NODE_ENV=development nodemon ./dist/server/index",
