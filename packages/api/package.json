    "@types/jsonwebtoken": "^8.3.2",
    "@types/murmurhash3js": "^3.0.2",
    "@types/request-promise-native": "^1.0.16",
    "@types/shortid": "^0.0.29",
    "@types/traverse": "^0.6.32",
    "@types/useragent": "^2.1.1",
    "@types/validator": "^10.11.2",
    "nodemon": "^1.19.1"
